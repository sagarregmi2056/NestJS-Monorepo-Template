{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "apps/api-server/src",
    "compilerOptions": {
        "deleteOutDir": true,
        "webpack": true,
        "tsConfigPath": "apps/api-server/tsconfig.app.json"
    },
    "monorepo": true,
    "root": "apps/api-server",
    "projects": {
        "api-server": {
            "type": "application",
            "root": "apps/api-server",
            "entryFile": "main",
            "sourceRoot": "apps/api-server/src",
            "compilerOptions": {
                "tsConfigPath": "apps/api-server/tsconfig.app.json"
            }
        },
        "worker": {
            "type": "application",
            "root": "apps/worker",
            "entryFile": "main",
            "sourceRoot": "apps/worker/src",
            "compilerOptions": {
                "tsConfigPath": "apps/worker/tsconfig.app.json"
            }
        },
        "websocket-service": {
            "type": "application",
            "root": "apps/websocket-service",
            "entryFile": "main",
            "sourceRoot": "apps/websocket-service/src",
            "compilerOptions": {
                "tsConfigPath": "apps/websocket-service/tsconfig.app.json"
            }
        },
        "admin": {
            "type": "application",
            "root": "apps/admin",
            "entryFile": "main",
            "sourceRoot": "apps/admin/src",
            "compilerOptions": {
                "tsConfigPath": "apps/admin/tsconfig.app.json"
            }
        },
        "db": {
            "type": "library",
            "root": "libs/db",
            "entryFile": "index",
            "sourceRoot": "libs/db/src",
            "compilerOptions": {
                "tsConfigPath": "libs/db/tsconfig.lib.json"
            }
        },
        "configuration": {
            "type": "library",
            "root": "libs/configuration",
            "entryFile": "index",
            "sourceRoot": "libs/configuration/src",
            "compilerOptions": {
                "tsConfigPath": "libs/configuration/tsconfig.lib.json"
            }
        },
        "common": {
            "type": "library",
            "root": "libs/common",
            "entryFile": "index",
            "sourceRoot": "libs/common/src",
            "compilerOptions": {
                "tsConfigPath": "libs/common/tsconfig.lib.json"
            }
        },
        "security": {
            "type": "library",
            "root": "libs/security",
            "entryFile": "index",
            "sourceRoot": "libs/security/src",
            "compilerOptions": {
                "tsConfigPath": "libs/security/tsconfig.lib.json"
            }
        },
        "swagger": {
            "type": "library",
            "root": "libs/swagger",
            "entryFile": "index",
            "sourceRoot": "libs/swagger/src",
            "compilerOptions": {
                "tsConfigPath": "libs/swagger/tsconfig.lib.json"
            }
        }
    }
}